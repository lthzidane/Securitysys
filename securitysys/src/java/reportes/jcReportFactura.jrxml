	<queryString>
		<![CDATA[select
LPAD(f.id_serie_comprobante::text, 3, '0') as serie, 
LPAD(f.nro_comprobante::text, 3, '0') as comp,
LPAD(f.id_venta::text, 3, '0') as NUM,
c.id_cliente as id, c.nombre, c.direccion, c.telefono as persona_telefono,
f.fecha_venta as f_Registro, u.nombre as vendedor, f.estado_venta as observacion,
p.id_equipo as IDP, p.descripcion as NameP,
d.cantidad, d.precio_venta as precioU,(d.cantidad * d.precio_venta) as total
from
venta f
inner join venta_det d on f.id_venta=d.id_venta
inner join cliente c on c.id_cliente=f.id_cliente
inner join equipo p on p.id_equipo=d.id_equipo
inner join usuario u on u.id_usuario=f.id_usuario
where f.id_venta=$P{NumFactura}]]>
	</queryString>
